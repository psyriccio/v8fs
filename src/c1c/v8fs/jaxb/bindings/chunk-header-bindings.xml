<?xml version="1.0" encoding="UTF-8"?>

<xml-bindings
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.eclipse.org/eclipselink/xsds/persistence/oxm'
    xsi:schemaLocation='http://www.eclipse.org/eclipselink/xsds/persistence/oxm ./eclipselink_oxm_2_4.xsd'>

    <java-types>

        <java-type name="c1c.v8fs.ChunkHeader" xml-accessor-type="FIELD">
            <java-attributes>
                <xml-attribute java-attribute="blockSize" name="blockSize" />
                <xml-attribute java-attribute="thisChunkSize" name="thisChunkSize" />
                <xml-attribute java-attribute="nextChunkPresent" name="nextChunkPresent" />
                <xml-attribute java-attribute="nextChunkAddress" name="nextChunkAddress" >
                    <xml-java-type-adapter value="c1c.v8fs.jaxb.AddressHexAdapter" />
                </xml-attribute>
                <xml-transient java-attribute="containerContext" />
            </java-attributes>
        </java-type>

    </java-types>

</xml-bindings>
