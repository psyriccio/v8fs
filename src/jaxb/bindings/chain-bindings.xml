<?xml version="1.0" encoding="UTF-8"?>

<xml-bindings
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.eclipse.org/eclipselink/xsds/persistence/oxm'
    xsi:schemaLocation='http://www.eclipse.org/eclipselink/xsds/persistence/oxm file:/home/psyriccio/v8fs/src/jaxb/bindings/eclipselink_oxm_2_4.xsd'>

    <java-types>

        <java-type name="c1c.v8fs.Chain" xml-accessor-type="FIELD">
            <java-attributes>
                <xml-attribute java-attribute="id" name="id" xml-id="true" />
                <xml-elements java-attribute="chunks">
                    <xml-element name="chunk" />
                </xml-elements>
                <xml-attribute java-attribute="address" name="address" type="java.lang.String" xml-accessor-type="PROPERTY">
                    <xml-access-methods get-method="getAddressXML" set-method="setAddressXML"/>
                </xml-attribute>
            </java-attributes>
        </java-type>

    </java-types>

</xml-bindings>
